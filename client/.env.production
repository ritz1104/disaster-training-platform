# Production Client Environment Variables

# API Configuration
VITE_API_URL=https://ndma-training-server.onrender.com/api
VITE_SOCKET_URL=https://ndma-training-server.onrender.com

# Maps Configuration
VITE_GOOGLE_MAPS_API_KEY=your-production-google-maps-api-key
VITE_MAPBOX_ACCESS_TOKEN=your-production-mapbox-access-token

# Environment
VITE_NODE_ENV=production

# Analytics
VITE_GOOGLE_ANALYTICS_ID=your-production-ga-tracking-id

# Error Tracking
VITE_SENTRY_DSN=your-production-sentry-dsn

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_OFFLINE_MODE=true

# App Configuration
VITE_APP_NAME=Disaster Training Platform
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=Comprehensive disaster preparedness training platform

# CDN and Assets
VITE_CDN_URL=https://cdn.your-domain.com
VITE_ASSETS_URL=https://assets.your-domain.com